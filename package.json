    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.2.0",
    "tsyringe": "^4.3.0",
    "typeorm": "^0.2.28",
    "typeorm-seeding": "^1.6.1",
    "typesense": "^0.12.1",
    "uuid": "^8.3.1",
    "winston": "^3.3.3"
  },
